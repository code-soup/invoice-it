{
	"name": "codesoup/invoice-plugin",
	"type": "wordpress-plugin",
	"license": "MIT",
	"description": "WordPress invoice plugin with Webpack dev server, Webpack build script and PSR-4 namespacing",
	"homepage": "https://github.com/code-soup/invoice-plugin",
    "support": {
        "issues": "https://github.com/code-soup/invoice-plugin/issues"
    },
	"authors": [
		{
			"name": "Vlado Bosnjak",
			"email": "vb@bobz.co",
			"homepage": "https://www.bobz.co"
		}
	],
	"keywords": [
        "codesoup",
		"wordpress",
        "wordpress plugin",
        "wordpress invoice plugin"
	],
    "autoload": {
        "psr-4": {
            "csip\\": "includes/"
        }
    },
	"require": {
		"php": ">=7.3.0",
		"composer/installers": "~1.0",
		"soberwp/models": "1.1.0-p",
		"htmlburger/carbon-fields": "~2.2.0",
		"rinvex/countries": "^7.0"
	},
	"extra": {
		"installer-paths": {
			"vendor/{$name}": ["vendor/package"]
		}
	}
}
